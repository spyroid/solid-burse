<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<g:HTMLPanel>
		<h3>
			<a ui:field="manufacturer"></a>
			<span ui:field="productName">
				MacbookPro
			</span>
		</h3>
		<table>
			<tr>
				<td valign="top">
					<g:Image ui:field="productImage"></g:Image>
				</td>
				<td valign="top">
					<h3>
						Average Price:
						<span ui:field="avgPrice">~2000$</span>
						<br />
						Offers to sell:
						<a ui:field="offersSell">+100500</a>
						<br />
						Offers to buy:
						<a ui:field="offersBuy">+100500</a>
					</h3>

				</td>
				<td valign="top">
					<h3>Price change over time</h3>
					<g:Image ui:field="trends"
						url="http://www.google.com/trends/viz?q=MacBook+Pro&amp;graph=weekly_img&amp;sa=N"></g:Image>
					<h3>Popularity</h3>
					<g:Image
						url="http://www.google.com/trends/viz?q=iPad&amp;graph=weekly_img&amp;sa=N"></g:Image>
					<h3>Another Stats</h3>
					<g:Image
						url="http://www.google.com/trends/viz?q=Spears&amp;graph=weekly_img&amp;sa=N"></g:Image>

				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 