<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:fl="urn:import:com.burse.client.ui.feed"
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>

  <ui:with field='res' type='com.burse.client.css.Resources' />

	<g:SplitLayoutPanel ui:field="feedSplitPanel"  width="100%" height="100%">
	    <g:west size="500">
		 	<g:DockLayoutPanel>
		 		<g:north size="30">
		 			<g:HTML horizontalAlignment="center"><a href="">2 new items</a></g:HTML>
		 		</g:north>
		 		<g:center>
		 			<fl:FeedList ui:field="feedList"/>
		 		</g:center>
		 	</g:DockLayoutPanel>
	    </g:west>
	    <g:center>
	        <fl:FeedInfo ui:field="feedInfo"></fl:FeedInfo>
	    </g:center>
	</g:SplitLayoutPanel>

</ui:UiBinder>
